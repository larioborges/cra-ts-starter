{
  "name": "@lario/local-proxy",
  "version": "1.0.0",
  "private": true,
  "description": "Proxy for running built web app locally",
  "author": "Lario Borges",
  "license": "UNLICENSED",
  "main": "src/index.js",
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@3.1.1",
  "scripts": {
    "prettier:format": "yarn prettier --write src/**/*.{js,jsx,ts,tsx,css,scss,less,sss,json}",
    "local:build": "cross-env API_PORT=4000 WEB_PORT=3000  nodemon ./src/index.js",
    "local:develop": "cross-env API_PORT=4000 WEB_PORT=3000  nodemon ./src/index.js"
  },
  "devDependencies": {
    "@lario/prettier-config": "workspace:^",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.0"
  },
  "prettier": "@lario/prettier-config",
  "dependencies": {
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "http-proxy-middleware": "^2.0.6"
  }
}
